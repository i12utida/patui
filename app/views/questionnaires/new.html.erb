<% provide(:title, 'アンケート投稿') %>
<h1>アンケート投稿</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@questionnaire) do |f| %>
    <%= render 'shared/questionnaire_form' %>

      <%= f.label :content %>
      <%= f.text_field :content, class: 'form-control' %>

      <%= f.submit "投稿する", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
